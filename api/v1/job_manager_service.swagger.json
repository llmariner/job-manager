{
  "swagger": "2.0",
  "info": {
    "title": "api/v1/job_manager_service.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "FineTuningService"
    },
    {
      "name": "FineTuningWorkerService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/fine_tuning/jobs": {
      "get": {
        "operationId": "FineTuningService_ListJobs",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1ListJobsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "after",
            "description": "after is the identifier for the last job from the previous pagination request.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "limit",
            "description": "limit is the number of fine-tuning jobs to retrieve. Defaults to 20.",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "FineTuningService"
        ]
      },
      "post": {
        "operationId": "FineTuningService_CreateJob",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Job"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1CreateJobRequest"
            }
          }
        ],
        "tags": [
          "FineTuningService"
        ]
      }
    },
    "/v1/fine_tuning/jobs/{id}": {
      "get": {
        "operationId": "FineTuningService_GetJob",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Job"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "FineTuningService"
        ]
      }
    },
    "/v1/fine_tuning/jobs/{id}/cancel": {
      "post": {
        "operationId": "FineTuningService_CancelJob",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1Job"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "FineTuningService"
        ]
      }
    }
  },
  "definitions": {
    "IntegrationWandb": {
      "type": "object",
      "properties": {
        "project": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "entity": {
          "type": "string"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "UpdateJobPhaseRequestPhase": {
      "type": "string",
      "enum": [
        "PHASE_UNSPECIFIED",
        "PREPROCESSED",
        "JOB_CREATED",
        "FINETUNED",
        "FAILED",
        "RECREATE",
        "CANCELED"
      ],
      "default": "PHASE_UNSPECIFIED"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "v1CreateJobRequest": {
      "type": "object",
      "properties": {
        "model": {
          "type": "string"
        },
        "trainingFile": {
          "type": "string"
        },
        "hyperparameters": {
          "$ref": "#/definitions/v1CreateJobRequestHyperparameters"
        },
        "suffix": {
          "type": "string",
          "description": "A string of up to 18 characters that will be added to your fine-tuned model name.\n\nFor example, a suffix of \"custom-model-name\" would produce a\nmodel name like\nft:gpt-3.5-turbo:openai:custom-model-name:7p4lURel."
        },
        "validationFile": {
          "type": "string"
        },
        "integrations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1Integration"
          }
        },
        "seed": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "v1CreateJobRequestHyperparameters": {
      "type": "object",
      "properties": {
        "batchSize": {
          "type": "integer",
          "format": "int32",
          "description": "Note: OpenAI API supports string or interger."
        },
        "learningRateMultiplier": {
          "type": "number",
          "format": "double",
          "description": "Note: OpenAI API supports string or number."
        },
        "nEpochs": {
          "type": "integer",
          "format": "int32",
          "description": "Note: OpenAI API supports string or interger."
        }
      }
    },
    "v1Integration": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "wandb": {
          "$ref": "#/definitions/IntegrationWandb"
        }
      }
    },
    "v1InternalJob": {
      "type": "object",
      "properties": {
        "job": {
          "$ref": "#/definitions/v1Job"
        },
        "outputModelId": {
          "type": "string"
        },
        "suffix": {
          "type": "string"
        },
        "state": {
          "$ref": "#/definitions/v1InternalJobState",
          "description": "state is also stored in the job object, but this value takes precedence."
        },
        "queuedAction": {
          "$ref": "#/definitions/v1InternalJobAction"
        }
      }
    },
    "v1InternalJobAction": {
      "type": "string",
      "enum": [
        "ACTION_UNSPECIFIED",
        "CREATING",
        "CANCELING"
      ],
      "default": "ACTION_UNSPECIFIED"
    },
    "v1InternalJobState": {
      "type": "string",
      "enum": [
        "STATE_UNSPECIFIED",
        "QUEUED",
        "RUNNING",
        "FAILED",
        "SUCCEEDED",
        "CANCELED"
      ],
      "default": "STATE_UNSPECIFIED"
    },
    "v1Job": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "int64"
        },
        "error": {
          "$ref": "#/definitions/v1JobError"
        },
        "fineTunedModel": {
          "type": "string",
          "description": "The name of the fine-tuned model that is being created. The value will be null if the fine-tuning job is still running."
        },
        "finishedAt": {
          "type": "string",
          "format": "int64"
        },
        "hyperparameters": {
          "$ref": "#/definitions/v1JobHyperparameters"
        },
        "model": {
          "type": "string",
          "description": "The base model that is being fine-tuned."
        },
        "object": {
          "type": "string"
        },
        "organizationId": {
          "type": "string"
        },
        "resultFiles": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "status": {
          "type": "string",
          "description": "The current status of the fine-tuning job, which can be either validating_files, queued, running, succeeded, failed, or cancelled."
        },
        "trainedTokens": {
          "type": "integer",
          "format": "int32"
        },
        "trainingFile": {
          "type": "string"
        },
        "validationFile": {
          "type": "string"
        },
        "integrations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1Integration"
          }
        },
        "seed": {
          "type": "integer",
          "format": "int32"
        },
        "projectId": {
          "type": "string"
        },
        "kubernetesNamespace": {
          "type": "string"
        },
        "clusterId": {
          "type": "string",
          "description": "cluster_id is the ID of the cluster where he job runs."
        },
        "organizationTitle": {
          "type": "string"
        },
        "projectTitle": {
          "type": "string"
        },
        "clusterName": {
          "type": "string"
        }
      }
    },
    "v1JobError": {
      "type": "object",
      "properties": {
        "code": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "param": {
          "type": "string"
        }
      }
    },
    "v1JobHyperparameters": {
      "type": "object",
      "properties": {
        "batchSize": {
          "type": "integer",
          "format": "int32",
          "description": "Note: OpenAI API supports string or interger."
        },
        "learningRateMultiplier": {
          "type": "number",
          "format": "double",
          "description": "Note: OpenAI API supports string or number."
        },
        "nEpochs": {
          "type": "integer",
          "format": "int32",
          "description": "Note: OpenAI API supports string or interger."
        }
      }
    },
    "v1ListJobsResponse": {
      "type": "object",
      "properties": {
        "object": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1Job"
          }
        },
        "hasMore": {
          "type": "boolean"
        }
      }
    },
    "v1ListQueuedInternalJobsResponse": {
      "type": "object",
      "properties": {
        "jobs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1InternalJob"
          }
        }
      }
    },
    "v1UpdateJobPhaseResponse": {
      "type": "object"
    }
  }
}
